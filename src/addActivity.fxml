<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="226.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddActivity">
   <children>
      <VBox prefHeight="400.0" prefWidth="240.0" AnchorPane.bottomAnchor="4.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="4.0">
         <children>
            <TableView fx:id="table" prefHeight="315.0" prefWidth="240.0">
              <columns>
                <TableColumn fx:id="activitiesCol" prefWidth="242.0" text="Activities" />
              </columns>
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="31.0" prefWidth="240.0">
               <children>
                  <TextField fx:id="newItem" prefHeight="33.0" prefWidth="120.0" />
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#addItem" prefHeight="33.0" prefWidth="120.0" text="Add Item">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </HBox>
            <HBox prefHeight="33.0" prefWidth="240.0">
               <children>
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#changeScene" prefHeight="33.0" prefWidth="120.0" text="Card">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#removeItem" prefHeight="33.0" prefWidth="120.0" text="Remove">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
